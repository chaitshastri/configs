[user]
	name = psachin
	email = iclcoolster@gmail.com
[core]
	editor = emacsclient -nw
	excludesfile = /home/sachin/.gitignore_global
	whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
;	gitproxy=http://10.101.11.108:3128
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	last = log --pretty=format:\"%h - %an, %ar : %s\"
	logg = log -p -l 2
	df = diff
	dc = diff --cached
        lg = log -p
        lol = log --graph --decorate --pretty=oneline --abbrev-commit
        lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
        ls = ls-files
	ign = ls-files -o -i --exclude-standard
[color]
	ui = true
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
[color "diff"]
        whitespace = red reverse	
[commit]
	template = /home/sachin/.gitmessage.txt
[http]
;	proxy = http://10.101.11.108:3128
	postBuffer = 524288000
[push]
	default = matching
